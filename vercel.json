{
    "version": 2,
    "builds": [
      {
        "src": "api/**/*.py",
        "use": "@vercel/python"
      },
      {
        "src": "main.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/api/extract_terms",
        "dest": "api/extract_terms.py"
      },
      {
        "src": "/api/generate-image",
        "dest": "api/image_generator.py"
      },
      {
        "src": "/(.*)",
        "dest": "main.py"
      }
    ]
  }
  